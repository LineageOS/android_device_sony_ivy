/boot        emmc /dev/block/bootdevice/by-name/boot       flags=display="BootKernel";backup=1
/FOTAKernel  emmc /dev/block/bootdevice/by-name/FOTAKernel flags=display="FOTAKernel";backup=1
#/ta          emmc /dev/block/bootdevice/by-name/TA         flags=display="Trim-Area";backup=1
/system      ext4 /dev/block/bootdevice/by-name/system     flags=display="System";backup=1
/data        ext4 /dev/block/bootdevice/by-name/userdata   length=-16384;flags=display="Data";retainlayoutversion
/cache       ext4 /dev/block/bootdevice/by-name/cache      flags=display="Cache"
/external_sd vfat /dev/block/mmcblk1p1                     flags=display="Micro SDcard";storage;removable;wipeingui
/usb-otg     vfat /dev/block/sda1                          flags=display="USB OTG";storage;removable;wipeingui
